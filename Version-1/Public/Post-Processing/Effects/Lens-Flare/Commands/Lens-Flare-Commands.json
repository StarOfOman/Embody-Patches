{
  "api": "Lens Flare",
  "transport": "Pixel Streaming DataChannel (WebRTC)",
  "endpoints": {
    "POST": [
      {
        "command": "CGG_LenseFlare_Intensity_{value}",
        "description": "Lens flare intensity",
        "params": { "value": "number - float 0 to 2, step 0.01" }
      },
      {
        "command": "CGG_LenseFlare_Tint_{value}",
        "description": "Lens flare tint",
        "params": { "value": "number - float 0 to 2, step 0.01" }
      },
      {
        "command": "CGG_LenseFlare_BokehSize_{value}",
        "description": "Lens flare bokeh size",
        "params": { "value": "number - float 0 to 10, step 0.1" }
      },
      {
        "command": "CGG_LenseFlare_Threshold_{value}",
        "description": "Lens flare threshold",
        "params": { "value": "number - float 0 to 2, step 0.01" }
      }
    ],
    "GET": []
  }
}
