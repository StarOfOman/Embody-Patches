{
  "api": "Film Tone Curve",
  "transport": "Pixel Streaming DataChannel (WebRTC)",
  "endpoints": {
    "POST": [
      {
        "command": "CGG_Misc_FilmSlope_{value}",
        "description": "Film tone curve slope",
        "params": { "value": "number - float 0 to 2, step 0.01" }
      },
      {
        "command": "CGG_Misc_FilmToe_{value}",
        "description": "Film tone curve toe",
        "params": { "value": "number - float 0 to 1, step 0.01" }
      },
      {
        "command": "CGG_Misc_FilmShoulder_{value}",
        "description": "Film tone curve shoulder",
        "params": { "value": "number - float 0 to 1, step 0.01" }
      },
      {
        "command": "CGG_Misc_FilmBlackClip_{value}",
        "description": "Film tone curve black clip",
        "params": { "value": "number - float 0 to 1, step 0.01" }
      },
      {
        "command": "CGG_Misc_FilmWhiteClip_{value}",
        "description": "Film tone curve white clip",
        "params": { "value": "number - float 0 to 1, step 0.01" }
      }
    ],
    "GET": []
  }
}
