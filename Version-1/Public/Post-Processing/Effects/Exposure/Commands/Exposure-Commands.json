{
  "api": "Exposure",
  "transport": "Pixel Streaming DataChannel (WebRTC)",
  "endpoints": {
    "POST": [
      {
        "command": "EXPO_Comp_{value}",
        "description": "Exposure compensation",
        "params": { "value": "number - float -5 to 5, step 0.01" }
      },
      {
        "command": "EXPO_MinEV100_{value}",
        "description": "Minimum exposure value (EV100)",
        "params": { "value": "number - float -10 to 0, step 0.1" }
      },
      {
        "command": "EXPO_MaxEV100_{value}",
        "description": "Maximum exposure value (EV100)",
        "params": { "value": "number - float 0 to 10, step 0.1" }
      },
      {
        "command": "EXPO_SpeedUp_{value}",
        "description": "Auto-exposure speed up (bright adaptation)",
        "params": { "value": "number - float 0.1 to 5, step 0.1" }
      },
      {
        "command": "EXPO_SpeedDown_{value}",
        "description": "Auto-exposure speed down (dark adaptation)",
        "params": { "value": "number - float 0.1 to 5, step 0.1" }
      }
    ],
    "GET": []
  }
}
