{
  "api": "Build Your Own World",
  "transport": "Pixel Streaming DataChannel (WebRTC)",
  "endpoints": {
    "POST": [
      {
        "command": "SPWN_{tag}_{Category.Object}_{x}_{y}_{z}_{rotX}_{rotY}_{rotZ}_{scaleX}_{scaleY}_{scaleZ}",
        "description": "Spawn a 3D object into the scene with full transform",
        "params": {
          "tag": "string - unique identifier for this object instance (no underscores allowed)",
          "Category.Object": "string - object type from the spawnable catalog (e.g. Furnish.BenchChair)",
          "x": "number - position X",
          "y": "number - position Y",
          "z": "number - position Z",
          "rotX": "number - rotation X (degrees)",
          "rotY": "number - rotation Y (degrees)",
          "rotZ": "number - rotation Z (degrees)",
          "scaleX": "number - scale X",
          "scaleY": "number - scale Y",
          "scaleZ": "number - scale Z"
        }
      },
      {
        "command": "SPWNMOVE_{tag}_{x}_{y}_{z}_{rotX}_{rotY}_{rotZ}_{scaleX}_{scaleY}_{scaleZ}",
        "description": "Move/transform an existing spawned object by its tag",
        "params": {
          "tag": "string - unique tag of the spawned object",
          "x": "number - new position X",
          "y": "number - new position Y",
          "z": "number - new position Z",
          "rotX": "number - new rotation X (degrees)",
          "rotY": "number - new rotation Y (degrees)",
          "rotZ": "number - new rotation Z (degrees)",
          "scaleX": "number - new scale X",
          "scaleY": "number - new scale Y",
          "scaleZ": "number - new scale Z"
        }
      },
      {
        "command": "SPWNDEL_{tag}",
        "description": "Delete a specific spawned object by its tag",
        "params": { "tag": "string - unique tag of the object to delete" }
      },
      {
        "command": "SPWND",
        "description": "Delete ALL spawned objects from the scene",
        "params": {}
      }
    ],
    "GET": []
  },
  "procedural_rock": {
    "description": "Procedural rock generation commands for Procedural.Rock objects. After spawning a Procedural.Rock, configure its base mesh and foliage layers.",
    "commands": [
      {
        "command": "PROCM_{tag}_{meshName}",
        "description": "Set base mesh for procedural rock",
        "options": ["ForestRock1", "ForestRock2", "ForestRock3", "ForestRock4", "ForestRock5", "ForestRock6", "ForestRock7", "ForestRock8", "ForestRock9", "ForestRock10"]
      },
      {
        "command": "PROCG_{tag}_{meshName}",
        "description": "Set grass mesh type (lowercase)",
        "options": ["Grass1", "Grass2", "Grass3"]
      },
      {
        "command": "PROCGD_{tag}_{value}",
        "description": "Set grass density",
        "params": "float 0.01 to 1, step 0.01"
      },
      {
        "command": "PROCGZ_{tag}_{value}",
        "description": "Set grass Z offset",
        "params": "float 0 to 1, step 0.01"
      },
      {
        "command": "PROCM_{tag}_Ferns_Mesh_{meshName}",
        "description": "Set ferns mesh type",
        "options": ["Fern1", "Fern2"]
      },
      {
        "command": "PROCFD_{tag}_{value}",
        "description": "Set ferns density",
        "params": "float 0 to 1, step 0.01"
      },
      {
        "command": "PROCFZ_{tag}_{value}",
        "description": "Set ferns Z offset",
        "params": "float 0 to 1, step 0.01"
      },
      {
        "command": "PROCR_{tag}_{meshName}",
        "description": "Set rocks mesh type (river rocks)",
        "options": ["RiverRock1", "RiverRock2", "RiverRock3", "RiverRockGroup"]
      },
      {
        "command": "PROCRD_{tag}_{value}",
        "description": "Set rocks density",
        "params": "float 0 to 1, step 0.01"
      },
      {
        "command": "PROCRZ_{tag}_{value}",
        "description": "Set rocks Z offset",
        "params": "float 0 to 1, step 0.01"
      },
      {
        "command": "PROCP_{tag}_{meshName}",
        "description": "Set pebbles mesh type",
        "options": ["Pebbles1", "Pebbles2"]
      },
      {
        "command": "PROCPD_{tag}_{value}",
        "description": "Set pebbles density",
        "params": "float 0 to 1, step 0.01"
      },
      {
        "command": "PROCPZ_{tag}_{value}",
        "description": "Set pebbles Z offset",
        "params": "float 0 to 1, step 0.01"
      },
      {
        "command": "PROCL_{tag}_{meshName}",
        "description": "Set logs mesh type",
        "options": ["Log1", "Log2", "Log3", "Log4", "Log5", "Log6"]
      },
      {
        "command": "PROCLD_{tag}_{value}",
        "description": "Set logs density",
        "params": "float 0 to 1, step 0.01"
      },
      {
        "command": "PROCLZ_{tag}_{value}",
        "description": "Set logs Z offset",
        "params": "float 0 to 1, step 0.01"
      }
    ]
  },
